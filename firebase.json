{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "rewrites": [
    {
      "source": "/assets/.env.prod",
      "destination": "/assets/.env.prod"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
}
